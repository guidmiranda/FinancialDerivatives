    "    dffloor['df'] = dffloor.end.apply(crv.df)\n",
    "    dffloor['fwd'] = dffloor.apply(lambda row: crv.fwd(row.start, row.end), axis=1)\n",
    "    dffloor['N'] = dffloor.start.apply(lambda x: (x - refDate).days / 360)\n",
    "\n",
    "    dffloor['d'] = -((dffloor.fwd - strike) / (normalvolcap * np.sqrt(dffloor.N)))\n",
    "    dffloor['caplet'] = dfcaps['caplet']\n",
    "    dffloor['floor'] = dffloor.notional * dffloor.dcf * dffloor.df * ((stk-dffloor.fwd) * norm.cdf(dffloor.d) \n",
    "                                   + normalvolcap * np.sqrt(dffloor.N) * norm.pdf(dffloor.d))\n",
    "\n",
    "\n",
    "    return dffloor.floor.sum()+dffloor.caplet.sum()\n",
    "\n",
    "\n",
    "#Error function\n",
    "def error (stk):\n",
    "    return ((floatLeg.pv.sum()-fstrike(stk))**2).sum()\n",
    "\n",
    "stk0 = 0.03\n",
    "\n",
    "#Minimizing the squared difference \n",
    "floorrate = minimize(error, stk0)['x']\n",
    "\n",
    "#Answer\n",
    "floorrate"
   ]

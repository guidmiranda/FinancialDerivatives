    "#2. a)\n",
    "\n",
    "#Calculating the Implied Fixed Rate from the PV of the FloatLeg (notional, dcf and df are the same)\n",
    "fixedRate1 = floatLeg.pv.sum() / (floatLeg.notional * floatLeg.dcf * floatLeg.df).sum()\n",
    "\n",
    "#Building the FixedLeg DataFrame\n",
    "fixedLeg1 = floatLeg[floatLeg.columns[:4]].copy()\n",
    "fixedLeg1['rate'] = fixedRate1\n",
    "fixedLeg1['cf'] = fixedLeg1.notional * fixedLeg1.dcf * fixedLeg1.rate\n",
    "fixedLeg1['df'] = floatLeg['df']\n",
    "fixedLeg1['pv'] = fixedLeg1.cf * fixedLeg1.df\n",
    "\n",
    "#Answer\n",
    "print(f\"Fixed Rate: {fixedRate1:.4%}\") \n",
    "print(f\"Fixed Leg PV: {fixedLeg1.pv.sum():,.2f}\")\n",
    "print(f\"Floating Leg PV: {floatLeg.pv.sum():,.2f}\")\n",
    "fixedLeg1.style.format(\"{:,.2f}\", subset=['notional', 'cf', 'pv'])"

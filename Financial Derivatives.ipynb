    "#FloatLeg Auxiliar DataFrame\n",
    "\n",
    "swapDates = makeSchedule(start, maturity)\n",
    "floatLeg1 = pd.DataFrame({\n",
    "    \"start\": swapDates[:-1],\n",
    "    \"end\": swapDates[1:],\n",
    "    \"notional\": notional\n",
    "})\n",
    "\n",
    "#Treating weekend days\n",
    "#Infer which weekday is each payment date (5 = Saturday; 6 = Sunday)\n",
    "floatLeg1['weekday_start'] = floatLeg1['start'].apply(lambda x: x.weekday())\n",
    "floatLeg1['weekday_end'] = floatLeg1['end'].apply(lambda x: x.weekday())\n",

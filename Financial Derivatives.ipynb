    "#2. g. Show a table for each of the swap legs with the future cashflows considering that today is \n",
    "#the 18th of December 2020\n",
    "\n",
    "#Loan info \n",
    "today=pd.to_datetime('2020-12-18')\n",
    "start=date(2020,7,20)\n",
    "end=date(2022,1,19)\n",
    "maturity=date(2022,1,19)\n",
    "euribor6MF =-0.0052 #fomos buscar manualmente a taxa\n",
    "\n",
    "#Filtering initial DataFrame to obtain the corresponding data from 18/12/2020\n",
    "\n",
    "data_loan2 = pd.DataFrame(data.loc['2020-12-18'])\n",
    "\n",
    "#Selecting the instruments to be used\n",
    "data_loan2.drop(data_loan2.index[1:6], inplace=True)\n",
    "data_loan2.drop(data_loan2.index[2:7], inplace=True)\n",
    "data_loan2.drop(data_loan2.index[4:20], inplace=True)\n",
    "\n",
    "#Instruments' maturities\n",
    "data_loan2['maturity'] = ['2021-06-18','2021-12-18','2022-06-18','2022-12-18']\n",
    "\n",
    "#Rearranging Dataframe\n",
    "data_loan2['maturity'] = pd.to_datetime(data_loan2['maturity'])\n",
    "data_loan2 = data_loan2.set_axis(['rates','maturity'],axis=1)\n",
    "data_loan2['rates']=data_loan2['rates']/100\n",
    "data_loan2['inst']=data_loan2.index\n",
    "data_loan2.reset_index(inplace=True)\n",
    "data_loan2=data_loan2.drop(columns='index')\n",
    "data_loan2=data_loan2[['inst','maturity','rates']] \n",
    "\n",
    "#Building a new Curve DataFrame\n",
    "\n",
    "spotDate2 = date(2020, 12, 18)\n",
    "\n",
    "curve2 = data_loan2\n",
    "\n",
    "curve2.loc[-1, ['inst','maturity']] = ['spot', datetime(2020, 12, 18)]\n",
    "curve2.loc[-1, 'df'] = 1\n",
    "curve2 = curve2.sort_index().reset_index(drop=True)\n",
    "\n",
    "for idx, row in curve2.iterrows():\n",
    "    if \"Deposit 6M\" in row.inst:\n",
    "        dcf = (row.maturity.date() - spotDate).days / 360\n",
    "        curve2.loc[idx, 'df'] = 1 / (1+row.rates * dcf)\n",
    "    if \"FRA\" in row.inst:\n",
    "        x = curve2[~curve2.df.isna()].maturity.apply(datetime.toordinal).values\n",
    "        y = curve2[~curve2.df.isna()].df\n",
    "        f = interp1d(x, y, kind='linear', fill_value=\"extrapolate\")\n",
    "        date0 = row.maturity + relativedelta(months=-6)\n",
    "        dcf = (row.maturity - date0).days / 360\n",
    "        df0 = f(date0.toordinal())\n",
    "        df1 = df0/(1+row.rates * dcf)\n",
    "        curve2.loc[idx, 'df'] = df1\n",
    "    if \"Swap\" in row.inst:\n",
    "        prev_dates = makeSchedule(today, row.maturity, 12)[1:-1]\n",
    "        x = curve2[~curve2.df.isna()].maturity.apply(datetime.toordinal).values\n",
    "        y = curve2[~curve2.df.isna()].df\n",
    "        f = interp1d(x, y, kind='linear', fill_value=\"extrapolate\")\n",
    "        prev_dfs = sum([f(dt.toordinal()) for dt in prev_dates])\n",
    "        curve2.loc[idx, 'df'] = (1 - row.rates * prev_dfs) / (1 + row.rates)\n",
    "            \n",
    "\n",
    "\n",
    "#Creating a new Curve Class\n",
    "\n",
    "today = date(2020,12,18)\n",
    "dates = curve2['maturity'].astype('str')\n",
    "discountFactors = curve2['df']\n",
    "\n",
    "class Curve2:\n",
    "    def __init__(self, refDate, dates, dfs):\n",
    "        self.refDate = refDate\n",
    "        self.dates = dates\n",
    "        self.times = [(datetime.strptime(dt, \"%Y-%m-%d\").date()-self.refDate).days / 360 for dt in dates]\n",
    "        self.dfs = np.array(dfs)\n",
    "    def __getDate(self, dt):\n",
    "            if isinstance(dt, str):\n",
    "                dt =  datetime.strptime(dt, '%Y-%m-%d').date()\n",
    "            return dt\n",
    "    def df(self, date):\n",
    "        date = self.__getDate(date)        \n",
    "        x = (date - self.refDate).days / 360\n",
    "        if x < 0:\n",
    "            return 0\n",
    "        return np.exp(np.interp(x, self.times, np.log(self.dfs)))\n",
    "    def fwd(self, start, end):\n",
    "        df0 = self.df(start)\n",
    "        df1 = self.df(end)\n",
    "        dcf = (self.__getDate(end) - self.__getDate(start)).days / 360\n",
    "        try:\n",
    "            return (df0 / df1 - 1) / dcf\n",
    "        except:\n",
    "            return 0\n",
    "    \n",
    "crv2 = Curve2(today, dates, discountFactors)\n",
    "\n",
    "#Buliding FloatLeg DataFrame\n",
    "\n",
    "floatLegFut = floatLeg.tail(3).copy()\n",
    "floatLegFut = floatLegFut[floatLegFut.columns[:2]]\n",
    "floatLegFut['notional'] = notional\n",
    "\n",
    "floatLegFut['dcf'] = floatLegFut.apply(lambda row: (row.end - row.start).days / 360, axis=1)\n",
    "floatLegFut['fwd'] = floatLegFut.apply(lambda row: crv2.fwd(row.start, row.end), axis=1)\n",
    "floatLegFut.loc[27, \"fwd\"] = euribor6MF\n",
    "floatLegFut['cf'] = floatLegFut.notional * floatLegFut.dcf * floatLegFut.fwd\n",
    "floatLegFut['df'] = floatLegFut.end.apply(crv.df)\n",
    "floatLegFut['pv'] = floatLegFut.cf * floatLegFut.df\n",
    "\n",
    "\n",
    "#Building FixedLeg DataFrame\n",
    "fixedRateFut = floatLegFut.pv.sum() / (floatLegFut.notional * floatLegFut.dcf * floatLegFut.df).sum()\n",
    "\n",
    "fixedLegFut = floatLegFut[floatLegFut.columns[:4]].copy()\n",
    "fixedLegFut['rate'] = fixedRateFut\n",
    "fixedLegFut['cf'] = fixedLegFut.notional * fixedLegFut.dcf * fixedLegFut.rate\n",
    "fixedLegFut['df'] = floatLegFut['df']\n",
    "fixedLegFut['pv'] = fixedLegFut.cf * fixedLegFut.df\n",
    "\n",
    "\n",
    "#Answer:\n",
    "display(fixedLegFut.style.format(\"{:,.2f}\", subset=['notional', 'cf', 'pv']))\n",
    "display(floatLegFut.style.format(\"{:,.2f}\", subset=['notional', 'cf', 'pv']))\n"
   ]

   "source": [
    "#1. What are the estimated cash flows of this loan on the 15th of January of 2007?\n",
    "\n",
    "#Creating the Curve Class\n",
    "today = date(2007,1,15)\n",
    "\n",
    "dates = curve['maturity'].astype('str')\n",
    "discountFactors = curve['df']\n",
    "\n",
    "class Curve:\n",
    "    def __init__(self, refDate, dates, dfs):\n",
    "        self.refDate = refDate\n",
    "        self.dates = dates\n",
    "        self.times = [(datetime.strptime(dt, \"%Y-%m-%d\").date()-self.refDate).days / 365.25 for dt in dates]\n",
    "        self.dfs = np.array(dfs)\n",
    "    def __getDate(self, dt):\n",
    "            if isinstance(dt, str):\n",
    "                dt =  datetime.strptime(dt, '%Y-%m-%d').date()\n",
    "            return dt\n",
    "    def df(self, date):\n",
    "        date = self.__getDate(date)        \n",
    "        x = (date - self.refDate).days / 365.25\n",
    "        if x < 0:\n",
    "            return 0\n",
    "        return np.exp(np.interp(x, self.times, np.log(self.dfs)))\n",
    "    def fwd(self, start, end):\n",
    "        df0 = self.df(start)\n",
    "        df1 = self.df(end)\n",
    "        dcf = (self.__getDate(end) - self.__getDate(start)).days / 360\n",
    "        try:\n",
    "            return (df0 / df1 - 1) / dcf\n",
    "        except:\n",
    "            return 0\n",
    "    \n",
    "crv = Curve(today, dates, discountFactors)\n",
    "\n",
    "#Buliding FloatLeg DataFrame\n",
    "floatLeg = pd.DataFrame({\n",
    "    \"start\": floatLeg1['start'],\n",
    "    \"end\": floatLeg1['end'],\n",
    "    \"notional\": notional\n",
    "})\n",
    "\n",
    "floatLeg['dcf'] = floatLeg.apply(lambda row: (row.end - row.start).days / 360, axis=1)\n",
    "floatLeg['fwd'] = floatLeg.apply(lambda row: crv.fwd(row.start, row.end), axis=1)\n",
    "floatLeg.loc[0, \"fwd\"] = euribor6M\n",
    "floatLeg['cf'] = floatLeg.notional * floatLeg.dcf * floatLeg.fwd\n",
    "floatLeg['df'] = floatLeg.end.apply(crv.df)\n",
    "floatLeg['pv'] = floatLeg.cf * floatLeg.df\n",
    "floatLeg.style.format(\"{:,.2f}\", subset=['notional', 'cf', 'pv'])\n",
    "\n",
    "# Answer\n",
    "print('The estimated cashflows of this loan are:')\n",
    "display(floatLeg.iloc[:,:6])"
   ]

    "normalVol = vol5y10y / 10000\n",
    "normalVol #Swaption ATM 5y10y Normal Vol 3.0402%\n",
    "\n",
    "#Option payment dates\n",
    "dates = pd.DataFrame({'Dates':floatLeg.iloc[-10:,0]})\n",

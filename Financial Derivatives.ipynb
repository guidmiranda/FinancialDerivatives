    "dates.loc[len(dates.index)]=[lastdate]\n",
    "dates = dates.reset_index(drop=True)\n",
    "\n",
    "#Auxiliary DataFrame to obtain correct dates\n",
    "b = pd.DataFrame({'Dates': floatLeg['start'], 'df':floatLeg['df']})\n",
    "dates2 = dates.merge(b, on = 'Dates')\n",
    "\n",
    "#Swap Rate (ATM)\n",
    "swapRate = ([dates2['df'].iloc[0] - dates2['df'].iloc[-1]] / np.sum(dates2['df'].iloc[1:]))[0]\n",
    "\n",
    "\n",
    "#Swaption\n",
    "A = sum(dates2['df'].iloc[1:])\n",
    "T = 5\n",
    "\n",
    "strike = swapRate\n",
    "d = (swapRate - strike)/(normalVol * np.sqrt(T))\n",
    "\n",
    "swaption = A * ((swapRate - strike)*norm.cdf(d) + normalVol * np.sqrt(T) * norm.pdf(d))\n",
    "\n",
    "L = notional\n",
    "swaption_value=swaption*L\n",
    "swaption_value\n",
